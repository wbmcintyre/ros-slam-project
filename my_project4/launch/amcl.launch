<launch>


  <!-- map server node -->
  <arg name="map_file" default="$(find my_project4)/maps/officeWorld.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)"/>

  <!-- amcl node -->
  <node name="amcl" pkg="amcl" type="amcl" output="screen">
    <param name="odom_frame_id" value="odom"/>
    <param name="odom_model_type" value="diff-corrected"/>
    <param name="base_frame_id" value="robot_footprint"/>
    <param name="global_frame_id" value="map"/>
    <param name="initial_pose_x" value="0"/>
    <param name="initial_pose_y" value="0"/>
    <param name="initial_pos_a" value="0"/>
    <param name="initial_cov_aa" value="0"/>
    <param name="transform_tolerance" value="0.1"/>
    <param name="max_particles" value="2000"/>
    <param name="min_particles" value="500"/>
    <param name="update_min_d" value="0.1"/>
    <param name="update_min_a" value="0.25"/>
    <param name="odom_alpha1" value="0.01"/>
    <param name="odom_alpha2" value="0.01"/>
    <param name="odom_alpha3" value="0.001"/>
    <param name="odom_alpha4" value="0.001"/>
  </node>

  <!-- include config files -->
  <rosparam file="$(find my_project4)/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
  <rosparam file="$(find my_project4)/config/costmap_common_params.yaml" command="load" ns="local_costmap"/>
  <rosparam file="$(find my_project4)/config/local_costmap_params.yaml" command="load"/>
  <rosparam file="$(find my_project4)/config/global_costmap_params.yaml" command="load"/>
  <rosparam file="$(find my_project4)/config/base_local_planner_params.yaml" command="load"/>

  <!-- move base node -->
  <node name="move_base" pkg="move_base" type="move_base" respawn="false" output="screen">
    <param name="base_global_planner" value="navfn/NavfnROS" />
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>
  </node>




</launch>
